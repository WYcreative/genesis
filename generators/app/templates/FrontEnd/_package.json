{
	"name": "<%= packageName %>",
	"version": "0.0.0",
	"description": "<%= description %>",
	"author": "WYcreative Engineering Team",
	"license": "<% if(type === 'website') { %>UNLICENSED<% } else { %>ISC<% } %>",
	<%_ if (type === 'website') { -%>
	"private": true,
	<%_ } -%>
	"type": "module",
	"engines": {
		"gulp-cli": ">=<%= gulpCliVersion %>",
		"node": ">=<%= nodeVersion %>",
		"npm": ">=<%= npmVersion %>"
	},
	"scripts": {
		"start": "gulp",
		"serve": "gulp serve",
		"test": "xo",
		"dist": "gulp dist",
		"clean": "gulp clean"<% if (type === 'website') { %>,
		"deploy": "gulp deploy"
		<%_ } %><% if (type === 'npm-package') { %>,
		"release": "np"
		<%_ } -%>
	},
	"repository": {
		"type": "git",
		"url": "git+<%= repository %>"
	},
	"homepage": "<%= homepage %>",
	"dependencies": {},
	"devDependencies": {},
	"browserslist": [
		"defaults and last 2 years",
		"not op_mob >= 0"
	],
	"babel": {
		"presets": [
			[
				"@babel/env",
				{
					"modules": false
				}
			]
		],
		"only": [
			"./@(config|data|src)/**"
		]
	},
	"xo": {
		"envs": [
			"browser",
			"node"
		],
		"rules": {
			"no-multiple-empty-lines": [
				"warn",
				{
				"max": 3
				}
			],
			"no-console": [
				"error",
				{
					"allow": [
						"info",
						"warn",
						"error"
					]
				}
			]
		}
	},
	<%_ if (type === "npm-package") { -%>
	"np": {
		"message": "DIST - v%s"
	},
	<%_ } -%>
	"genesis": {
		"version": "<%= genesisVersion %>"
	}
}
