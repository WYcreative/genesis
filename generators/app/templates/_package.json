{
	"name": "<%= packageName %>",
	"version": "0.0.0",
	"description": "<%= description %>",
	"author": "WYcreative Engineering Team",
	"license": "UNLICENSED",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=<%= nodeVersion %>",
		"npm": ">=<%= npmVersion %>"
	},
	"scripts": {
		"start": "gulp",
		"serve": "gulp serve",
		"test": "xo",
		"dist": "gulp dist",
		"clean": "gulp clean",
		"deploy": "gulp deploy"
	},
	"repository": {
		"type": "git",
		"url": "git+https://dev.azure.com/Bycom/_git/<%= repository %>"
	},
	"homepage": "<%= homepage %>",
	"dependencies": {},
	"devDependencies": {},
	"browserslist": [
		"defaults",
		"not IE 11"
	],
	"babel": {
		"presets": [
			[
				"@babel/env",
				{
					"modules": false
				}
			]
		],
		"only": [
			"./@(config|data|src)/**"
		]
	},
	"xo": {
		"envs": [
			"browser",
			"node"
		],
		"rules": {
			"no-multiple-empty-lines": [
				1,
				{
					"max": 3
				}
			]
		}
	},
	"starter": {
		"version": "<%= starterVersion %>"
	}
}
