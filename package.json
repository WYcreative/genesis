{
	"name": "@wycreative/generator-starter",
	"version": "1.1.0",
	"description": "The WYcreative Starter",
	"keywords": [
		"yeoman-generator"
	],
	"type": "module",
	"exports": "./generators/app/index.js",
	"engines": {
		"gulp-cli": ">=2.3",
		"node": ">=18",
		"npm": ">=8.6"
	},
	"scripts": {
		"test": "xo",
		"release": "np"
	},
	"files": [
		"generators",
		".npmrc"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WYcreative/starter.git"
	},
	"author": "WYcreative Engineering Team",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/WYcreative/starter/issues"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"homepage": "https://github.com/WYcreative/starter#readme",
	"dependencies": {
		"@sindresorhus/slugify": "^2.1.1",
		"@wycreative/team": "^1.5.0",
		"chalk": "^5.1.0",
		"package-json": "^8.1.0",
		"semver": "^7.3.8",
		"validate-npm-package-name": "^4.0.0",
		"yeoman-generator": "^5.7.0",
		"yosay": "^2.0.2"
	},
	"devDependencies": {
		"np": "^7.6.2",
		"xo": "^0.52.4"
	},
	"xo": {
		"rules": {
			"no-multiple-empty-lines": [
				"warn",
				{
				"max": 3
				}
			],
			"no-console": [
				"error",
				{
					"allow": [
						"info",
						"warn",
						"error"
					]
				}
			]
		},
		"ignores": [
			"generators/app/templates/FrontEnd/config/index.js",
			"generators/app/templates/FrontEnd/config/guide/index.js",
			"generators/app/templates/FrontEnd/src/**"
		]
	},
	"np": {
		"message": "DIST - v%s"
	}
}
