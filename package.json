{
	"name": "@wycreative/generator-genesis",
	"version": "3.4.0",
	"description": "Genesis, the WYcreative Engineering Team Front-End project generator.",
	"keywords": [
		"yeoman-generator"
	],
	"type": "module",
	"exports": "./index.js",
	"engines": {
		"gulp-cli": ">=2.3",
		"node": ">=18.17",
		"npm": ">=9.6"
	},
	"scripts": {
		"test": "xo",
		"release": "np"
	},
	"files": [
		"index.js",
		"generators",
		".npmrc"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WYcreative/genesis.git"
	},
	"author": "WYcreative Engineering Team",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/WYcreative/genesis/issues"
	},
	"homepage": "https://github.com/WYcreative/genesis#readme",
	"dependencies": {
		"@sindresorhus/slugify": "^2.2.1",
		"chalk": "^5.3.0",
		"package-json": "^8.1.1",
		"semver": "^7.5.4",
		"validate-npm-package-name": "^5.0.0",
		"yeoman-generator": "^7.1.1",
		"yosay": "^3.0.0"
	},
	"devDependencies": {
		"np": "^8.0.4",
		"xo": "^0.56.0"
	},
	"xo": {
		"rules": {
			"no-multiple-empty-lines": [
				"warn",
				{
					"max": 3
				}
			],
			"import/order": [
				"error",
				{
					"newlines-between": "always"
				}
			],
			"no-console": [
				"error",
				{
					"allow": [
						"info",
						"warn",
						"error"
					]
				}
			]
		},
		"ignores": [
			"generators/*/templates/**"
		]
	},
	"np": {
		"message": "DIST - v%s"
	}
}
